<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.conservatoire.HoraireController">
   <children>
      <Label layoutX="304.0" layoutY="102.0" text="Lundi : 10:00 - 14:30">
